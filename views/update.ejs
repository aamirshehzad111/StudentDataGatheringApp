

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title></title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" type="text/css" media="screen" href="main.css">
    <script src="main.js"></script>
</head>
<body>
    
<div>
     <form action="/update-data" method='post'>
        <table border='2px' width='500px' height='200px'>
        <thead style='background-color:yellow'>
        <th colspan=5 >Data Gathering App</th>
        </thead>
        <% for(var i=0; i<studentData.length; i++) {%>
        <tbody align='center'>
        <tr>
        <td>Name:</td>
        <td><input type="text" name="sname" id="name" value="<%= studentData[i].sname %>"  ></td>
        </tr>
        <tr>
        <td>Father name:</td>
        <td><input type="text" name="fname" id="fname" value="<%= studentData[i].fname %>"  ></td>
        </tr>

        <tr>
        <td>Univeristy:</td>
        <td><input type="text" name="uni" id="uni" value="<%= studentData[i].uni %>" ></td>
        </tr>


        <tr>
        <td>Dept:</td>
        <td><input type="text" name="dept" id="dept" value="<%= studentData[i].dept %>"  ></td>
        </tr>

        <tr>
        <td>Degrree:</td>
        <td><select name="deg" id="deg" value="<%= studentData[i].deg %>"  >
        <option value="None" selected disabled></option>
        <option value="Master">Master</option>
        <option value="Bachalor">Bachalor</option>
        </select></td>
        </tr>

        <tr>
        <td>CGPA:</td>
        <td><input type="text" name="cgpa" id="cgpa" value="<%= studentData[i].cgpa %>" ></td>
        </tr>

        <tr>
        
        <td colspan=5><input type="submit" value="submit"></td>
        </tr>

        </tbody>
        <% } %>
        </table>

     </form>
     </div>
     

</body>
</html>
